C  UTLDAT.H - 1D TABLE DATA

      REAL*8 TABX,TABY,DYDX,TRANC1,VALUES,XYZERO
      INTEGER NTBU,NEXFRE,INTRPS,KXTRPS,INDEX1,INDEX2,INDEXL,KTRANS
      LOGICAL ISUNTDT
C
      COMMON /TABLES/ TABX,TABY,DYDX,TRANC1($MXTAB),VALUES($MXTBVL),
     & XYZERO($MXTAB),NTBU,NEXFRE,ISUNTDT,INTRPS($MXTAB),KXTRPS($MXTAB),
     & INDEX1($MXTAB),INDEX2($MXTAB),INDEXL($MXTAB),KTRANS($MXTAB)
